<!DOCTYPE HTML>
<!--
tinypng
-->
<html>

<head>
	<title>
		Code Book
	</title>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
	<!-- <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css"> -->

	<link rel="stylesheet" href="/assets/css/bootstrap.min.css">
	<link rel="stylesheet" href="/assets/css/main.css" />

	<script src="https://kit.fontawesome.com/85999f6d09.js" crossorigin="anonymous"></script>

</head>

<body class="">
	<!-- Nav tabs -->
	<%- include('./partials/nav.ejs') %>

		<section>
			<div class="row">
				<div class="col py-3" style="background-color: #fff;">
					<div class="container">
						<h2 class="display-4"><i class="fas fa-code" style="font-size: 45px;"></i>Contest 1</h2>
					</div>

				</div>
			</div>
			<div class="row mt-2">
				<div class="col-md-10 mx-md-auto">
                    <table class="table table-bordered table-striped table-dark justify-content-between">
                        <thead>
                          <tr>
                            <th scope="col">#</th>
                            <th scope="col">Handle</th>
                            <th scope="col">Score</th>
                          </tr>
                        </thead>
                        <tbody>
                          <%standing.forEach( (p,index) => {%>
                          <tr>
                            <th scope="row"><%=index+1%></th>
                            <td><%=p.Participant%></td>
                            <td><%=p.Score%></td>
                          </tr>
                          <%});%>
                        </tbody>
                      </table>
				</div>
			</div>

		</section>
		<%- include('./partials/footer.ejs') %>
		<!-- Scripts -->

		<%- include('./partials/Scripts.ejs') %>

		<script>


		</script>
</body>

</html>